<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_info">

    <fragment
        android:id="@+id/navigation_info"
        android:tag = "@string/check"
        android:name="com.example.elebicycle.ui.bicycleInfo.InfoFragment"
        android:label="信息显示"
        tools:layout="@layout/info_fragment"
        >

    </fragment>


    <fragment
        android:id="@+id/navigation_record"
        android:name="com.example.elebicycle.ui.record.RecordFragment"
        android:label="行车记录"
        tools:layout="@layout/record_fragment" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.elebicycle.ui.setting.SettingFragment"
        android:label="设置中心"
        tools:layout="@layout/setting_fragment" />

</navigation>